{
  "expo": {
    "name": "",
    "slug": "goldensignature",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fasalurahman.goldensignature"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fasalurahman.goldensignature",
      "permissions": ["android.permission.ACCESS_NETWORK_STATE"],
      "jsEngine": "hermes",
      "buildType": "aab",
      "enableDangerousExperimentalLeanBuilds": true,
      "splits": {
        "abi": true,
        "density": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to your device",
          "isAccessMediaLocationEnabled": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d554a1aa-540e-4c03-b505-f5e6e3bf2381"
      }
    }
  }
}
